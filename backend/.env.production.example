# Production Environment Configuration

# LLM Configuration
LLM_BACKEND=vllm
LLM_MODEL=Qwen/Qwen3-8B

# vLLM Configuration
VLLM_BASE_URL=http://vllm:8100/v1
VLLM_API_KEY=your-secure-api-key
VLLM_MODEL=Qwen/Qwen3-8B
VLLM_GPU_MEMORY_UTILIZATION=0.9
